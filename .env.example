# database postgresql url
DATABASE_URL=postgresql://[USERNAME]:[PASSWORD]@[HOST_URL]/[DATABASE]

# node environment
NODE_ENV=development # or production

# n8n Webhook URLs - Development
PARTS_SEARCH_WEBHOOK_URL=
QUOTE_REQUEST_WEBHOOK_URL=
EMAIL_PARSER_WEBHOOK_URL=
FOLLOW_UP_WEBHOOK_URL=
ORDER_CONFIRMATION_WEBHOOK_URL=
CUSTOMER_SUPPORT_WEBHOOK_URL=
PRICE_UPDATE_WEBHOOK_URL=
POST_ORDER_WEBHOOK_URL=
ORDER_FOLLOW_UP_WEBHOOK_URL=
N8N_BASE_URL=

# n8n Webhook URLs - Production (commented out by default)
# PARTS_SEARCH_WEBHOOK_URL=
# QUOTE_REQUEST_WEBHOOK_URL=
# EMAIL_PARSER_WEBHOOK_URL=
# FOLLOW_UP_WEBHOOK_URL=
# ORDER_CONFIRMATION_WEBHOOK_URL=
# CUSTOMER_SUPPORT_WEBHOOK_URL=

# Authentication
NEXTAUTH_URL=
NEXTAUTH_SECRET=
SECRET_KEY=
N8N_WEBHOOK_SECRET=

# Email Service (for password reset, verification)
EMAIL_SERVER_HOST="smtp.example.com"
EMAIL_SERVER_PORT=587
EMAIL_SERVER_USER="your-email@example.com"
EMAIL_SERVER_PASSWORD="your-email-password"
EMAIL_FROM="noreply@yourdomain.com"

# Feature Flags
ENABLE_TWO_FACTOR_AUTH=false
ENABLE_SOCIAL_LOGIN=false

# Logging
LOG_LEVEL="info" # debug, info, warn, error

# AWS S3 Configuration (if applicable)
AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_S3_BUCKET_NAME=
AWS_REGION=

